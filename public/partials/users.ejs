<div class="container" ng-controller="usersController as usersCtrl">
    <div class="panel panel-default">
        
        <div class="panel-heading">
            <h3 class="panel-title">Users</h3>
        </div>
        
        <div class="panel-body">
            <div ng-repeat="data in users">
                <ng-user data="data" user="user"></ng-user>
            </div>
        </div>
    </div>
</div>